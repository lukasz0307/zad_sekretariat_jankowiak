                            <Label Background="LightBlue">Imię</Label>
                            <TextBox Name="imie_ucz" Grid.Column="1"></TextBox>
                            <Label Background="LightBlue" Grid.Row="1">Drugie imie</Label>
                            <TextBox  Name="drugie_imie_ucz" Grid.Column="1" Grid.Row="1"></TextBox>
                            <Label Background="LightBlue" Grid.Row="2">Nazwisko</Label>
                            <TextBox Name="nazwisko_ucz" Grid.Column="1" Grid.Row="2"></TextBox>
                            <Label Background="LightBlue" Grid.Row="3">Nazwisko panieńskie</Label>
                            <TextBox Name="nazwisko_panienskie_ucz" Grid.Column="1" Grid.Row="3"></TextBox>
                            <Label Background="LightBlue" Grid.Row="4">Imiona rodziców</Label>
                            <TextBox Name="imiona_rodzicow_ucz" Grid.Column="1" Grid.Row="4"></TextBox>
                            <Label Background="LightBlue" Grid.Row="5">Data urodzenia(dzień.miesiąc.rok)</Label>
